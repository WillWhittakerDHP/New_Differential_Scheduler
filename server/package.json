{
  "name": "server",
  "main": "./server.js",
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "npm run build && node dist/server.js --ignore client",
    "dev": "npx nodemon  && run seed",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "node dist/seeds/index.js"
